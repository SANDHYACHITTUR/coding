#include<stdio.h>
#include<conio.h>

int fact(int);

void main()
{
	int no,factorial;
 
  	printf("Enter a number:");
  	scanf("%d",&no);
  	factorial=fact(no);
    printf("%d\n",factorial);
}

int fact(int n)
{
    int i,f=1;
    for(i=1;i<=n;i++)
    {
        f=f*i;
    }
    return f;
}
